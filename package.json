{
  "name": "patientoutcome-backend",
  "version": "0.4.0",
  "description": "Backend for patient outcome app.",
  "author": "Victor Cighir",
  "repository": "edwinhern/express-typescript-2024",
  "license": "MIT",
  "main": "index.ts",
  "private": true,
  "scripts": {
    "dev": "tsx watch --clear-screen=false src/index.ts | pino-pretty",
    "build": "tsup",
    "build:types": "node scripts/copy-types.js",
    "start": "node dist/index.js",
    "clean": "rimraf dist coverage",
    "lint": "biome check src/",
    "lint:fix": "biome check src/ --fix",
    "format": "biome format src/",
    "test": "vitest run",
    "type-check": "tsc --noEmit",
    "prepare": "husky",
    "docs": "jsdoc -c jsdoc.json",
    "docs:serve": "npx http-server docs -p 8080 -o"
  },
  "dependencies": {
    "@asteasolutions/zod-to-openapi": "^7.3.4",
    "@aws-sdk/client-s3": "^3.964.0",
    "@zodyac/zod-mongoose": "^3.2.0",
    "archiver": "^7.0.1",
    "bcrypt": "^6.0.0",
    "connect-mongo": "^6.0.0",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dayjs": "^1.11.19",
    "dotenv": "^17.2.3",
    "envalid": "^8.1.1",
    "express": "^5.2.1",
    "express-acl": "^2.0.9",
    "express-rate-limit": "^8.2.1",
    "express-session": "^1.18.2",
    "helmet": "^8.1.0",
    "http-status-codes": "^2.3.0",
    "mongoose": "^8.21.0",
    "multer": "^2.0.2",
    "node-cron": "^4.2.1",
    "nodemailer": "^7.0.12",
    "pino": "^10.1.0",
    "pino-http": "^11.0.0",
    "ssh2-sftp-client": "^12.0.1",
    "swagger-ui-express": "^5.0.1",
    "tar": "^7.5.2",
    "uuid": "^13.0.0",
    "webdav": "^5.8.0",
    "winston": "^3.19.0",
    "winston-daily-rotate-file": "^5.0.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.11",
    "@faker-js/faker": "^10.2.0",
    "@types/archiver": "^7.0.0",
    "@types/bcrypt": "^6.0.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/express-session": "^1.18.2",
    "@types/multer": "^2.0.0",
    "@types/node-cron": "^3.0.11",
    "@types/nodemailer": "^7.0.4",
    "@types/ssh2-sftp-client": "^9.0.6",
    "@types/supertest": "^6.0.3",
    "@types/swagger-ui-express": "^4.1.8",
    "@types/tar": "^6.1.13",
    "better-docs": "^2.7.3",
    "biome": "^0.3.3",
    "docdash": "^2.0.2",
    "husky": "^9.1.7",
    "jsdoc": "^4.0.5",
    "lint-staged": "^16.2.7",
    "pino-pretty": "^13.1.3",
    "rimraf": "^6.1.2",
    "supertest": "^7.2.2",
    "tinypool": "^2.1.0",
    "tsup": "^8.5.1",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vite-tsconfig-paths": "^6.0.3",
    "vitest": "^4.0.16",
    "zod-to-json-schema": "^3.25.1"
  },
  "lint-staged": {
    "*.{js,ts,cjs,mjs,d.cts,d.mts,json,jsonc}": [
      "biome check --write --no-errors-on-unmatched"
    ]
  },
  "tsup": {
    "entry": [
      "src",
      "!src/**/__tests__/**",
      "!src/**/*.test.*",
      "!src/**/*.yaml.*",
      "!src/**/*.md"
    ],
    "splitting": false,
    "sourcemap": true,
    "clean": true
  }
}
