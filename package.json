{
  "name": "patientoutcome-backend",
  "version": "0.4.0",
  "description": "Backend for patient outcome app.",
  "author": "Victor Cighir",
  "repository": "edwinhern/express-typescript-2024",
  "license": "MIT",
  "main": "index.ts",
  "private": true,
  "scripts": {
    "dev": "tsx watch --clear-screen=false src/index.ts | pino-pretty",
    "build": "tsup",
    "build:types": "node scripts/copy-types.js",
    "start": "node dist/index.js",
    "clean": "rimraf dist coverage",
    "lint": "biome check src/",
    "lint:fix": "biome check src/ --fix",
    "format": "biome format src/",
    "test": "vitest run",
    "prepare": "husky",
    "docs": "jsdoc -c jsdoc.json",
    "docs:serve": "npx http-server docs -p 8080 -o"
  },
  "dependencies": {
    "@asteasolutions/zod-to-openapi": "^7.3.4",
    "@zodyac/zod-mongoose": "^3.2.0",
    "bcrypt": "^6.0.0",
    "connect-mongo": "^5.1.0",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dayjs": "^1.11.13",
    "dotenv": "^16.6.1",
    "envalid": "^8.1.0",
    "express": "^4.21.2",
    "express-acl": "^2.0.9",
    "express-rate-limit": "^7.5.1",
    "express-session": "^1.18.2",
    "helmet": "^8.1.0",
    "http-status-codes": "^2.3.0",
    "mongoose": "^8.17.1",
    "nodemailer": "^7.0.11",
    "pino": "^9.8.0",
    "pino-http": "^10.5.0",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^11.1.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@faker-js/faker": "^9.9.0",
    "@types/bcrypt": "^6.0.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.23",
    "@types/express-session": "^1.18.2",
    "@types/nodemailer": "^7.0.4",
    "@types/supertest": "^6.0.3",
    "@types/swagger-ui-express": "^4.1.8",
    "better-docs": "^2.7.3",
    "biome": "^0.3.3",
    "docdash": "^2.0.2",
    "husky": "^9.1.7",
    "jsdoc": "^4.0.5",
    "lint-staged": "^15.5.2",
    "pino-pretty": "^11.3.0",
    "rimraf": "^6.0.1",
    "supertest": "^7.1.4",
    "tinypool": "^1.1.1",
    "tsup": "^8.5.0",
    "tsx": "^4.20.3",
    "typescript": "^5.9.2",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^4.0.15",
    "zod-to-json-schema": "^3.0.0"
  },
  "lint-staged": {
    "*.{js,ts,cjs,mjs,d.cts,d.mts,json,jsonc}": [
      "biome check --write --no-errors-on-unmatched"
    ]
  },
  "tsup": {
    "entry": [
      "src",
      "!src/**/__tests__/**",
      "!src/**/*.test.*",
      "!src/**/*.yaml.*",
      "!src/**/*.md"
    ],
    "splitting": false,
    "sourcemap": true,
    "clean": true
  }
}
